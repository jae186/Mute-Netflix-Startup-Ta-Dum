{
    "manifest_version":3,
    "name": "Mute Netflix Startup Sound 'Ta-Dum'",
    "description": "Mutes the Netflix sound at the beginning of each episode",
    "version": "1.0",

    "action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
		"tabs",
        "storage"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
      ],
    "background": {
        "service_worker": "background-worker.js"
      }
}