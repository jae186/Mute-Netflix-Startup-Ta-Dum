{
    "manifest_version":3,
    "name": "Muter",
    "description": "Mutes",
    "version": "1.0",

    "action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
		"webNavigation",
		"tabs",
        "scripting"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
      ],
    "background": {
        "service_worker": "background-worker.js"
      }
}